{
  "expo": {
    "name": "Journey to Australia",
    "slug": "journey-to-australia",
    "version": "1.0.2",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/appIcon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#1267E9"
    },
    "ios": {
      "bundleIdentifier": "org.name.journeytoaustralia",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to let you share them with your friends.",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptive-icon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "white"
      },
      "googleServicesFile": "./google-services.json",
      "package": "org.name.journeytoaustralia"
    },
    "extra": {
      "eas": {
        "projectId": "9efab9bf-b8c5-43ec-819d-1a007205ee2e"
      }
    },
    "owner": "startek"
  },
  "name": "Journey to Australia"
}
